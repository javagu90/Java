package tms_incidencias.modelo;

/**
 *Permite un mejor manejo del objeto
 * para las actividades disponibles
 * para la interfaz grafica
 * @author Osvaldo Sanchez
 */
public class ActividadAdicional implements java.io.Serializable{

    private Long id;
    private String claveIncidencia;
    private String incidencia;

    public ActividadAdicional() {
    }

    public String getClaveIncidencia() {
        return claveIncidencia;
    }

    public void setClaveIncidencia(String claveIncidencia) {
        this.claveIncidencia = claveIncidencia;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getIncidencia() {
        return incidencia;
    }

    public void setIncidencia(String incidencia) {
        this.incidencia = incidencia;
    }
     @Override
    public String toString() {
        return incidencia;
    }
}
