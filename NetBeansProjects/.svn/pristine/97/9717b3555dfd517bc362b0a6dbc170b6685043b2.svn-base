    }

    private void cargarEmpresas() {
        List<TmsEmpresasTbl> empresas = new ArrayList<TmsEmpresasTbl>();
        empresas = empresaFacateRemote.findAll();
        TmsEmpresasTbl em = new TmsEmpresasTbl();
        em.setEmpresaId(new BigDecimal("99999"));
        em.setEmpresaNombre("Todas");
        em.setEmpresaNombreCorto("Todas");
        em.setDescripcion("Todas");
        empresas.add(0,em);
        EmpresasComboBoxModel empresaModelo = new EmpresasComboBoxModel(empresas);
        setEmpresasModelo(empresaModelo);
    }

    private void setEmpresasModelo(EmpresasComboBoxModel empresaModelo) {
        jCmbEmpresa.setModel(empresaModelo);
        jCmbEmpresa.setSelectedIndex(0);
    }
    
