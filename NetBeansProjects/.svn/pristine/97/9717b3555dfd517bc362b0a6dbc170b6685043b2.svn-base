    private boolean ejecutaReporteJasper(){
        Map param = new HashMap();
        param.put("FechaInicial",jTxtFi.dateEditor.getTexto());        
        param.put("FechaFinal",jTxtFf.dateEditor.getTexto());   
        System.out.println("Fecha Inicial:"+jTxtFi.dateEditor.getTexto());
        System.out.println("Fecha Inicial:"+jTxtFf.dateEditor.getTexto());
        if(jCmbServicio.getSelectedItem().toString().equals("Todos"))
        {
            System.out.println("Manda el servcio null");
            param.put("NumeroServicio",null);
        }
        else
            param.put("NumeroServicio",((ServiciosComboBoxModel)jCmbServicio.getModel()).getServicio(jCmbServicio.getSelectedIndex()).getServicioNumero());
        if(jCmbEmpresa.getSelectedItem().toString().equals("Todas"))
        {
            System.out.println("Manda empresa null");
           param.put("NombreEmpresa",null);
        }
        else
            param.put("NombreEmpresa",((EmpresasComboBoxModel)jCmbEmpresa.getModel()).getEmpresa(jCmbEmpresa.getSelectedIndex()).getEmpresaNombre());            
        
        if (jTxtAutobus.getText().equals("")) {
            System.out.println("AutobusId: "+null);
            param.put("AutobusId",null);
        }
        else {
            Vector autobusId = servicioFacadeRemote.getAutobusId(jTxtAutobus.getText());
            if (autobusId.size() == 0) {
                new JDlgAceptar("Ejecución de Reportes","El número de autobús no existe.",Color.RED).setVisible(true);
                jTxtAutobus.requestFocus();
                return false;
            }
            else{
                Vector autobus = (Vector)autobusId.get(0);
                param.put("AutobusId",autobus.get(0).toString());
            }
        }
         
        if (jTxtOperador.getText().equals("")){
            System.out.println("OperadorId: "+null);
            param.put("OperadorId",null);
        }
        else {
            Vector operadorId = servicioFacadeRemote.getOperadorId(jTxtOperador.getText());
            if (operadorId.size() == 0) {
                new JDlgAceptar("Ejecución de Reportes","La clave de operador no existe.",Color.RED).setVisible(true);
                jTxtOperador.requestFocus();
                return false;
            }
            else{
                Vector operador = (Vector)operadorId.get(0);
                param.put("OperadorId",operador.get(0).toString());
            }
        }
       boolean regresa = true;
        if(reporte.equals("Monitor: Venta por Ruta"))
        {
           System.out.println("ejecuta el reporte: Monitor: Venta por Ruta");
           regresa = jClsImpresionReporte.imprimirReporte((HashMap) param, "Reportes/VentaDiaria/VentasXserviciosXruta.jasper");
        }
        if(reporte.equals("Monitor: FIK"))
        {
           System.out.println("ejecuta el reporte: Monitor: FIK");
           regresa = jClsImpresionReporte.imprimirReporte((HashMap) param, "Reportes/Fik/FIK_full.jasper");
        }
        
       return regresa;
    }

    private boolean validaParametros() {
        if (jTxtFi.dateEditor.getTexto().equals("") || jTxtFf.dateEditor.getTexto().equals("")){
            JDlgAceptar error1Dlg = new JDlgAceptar("Ejecución de Reportes","La valores de Fecha Inicial y Fecha Final son obligatorios.",Color.RED);
            error1Dlg.setVisible(true);
            return false;
        }
        if (jTxtFi.getDate().getTime() > jTxtFf.getDate().getTime()){
            JDlgAceptar error2Dlg = new JDlgAceptar("Ejecución de Reportes","La Fecha Inicial debe ser menor a la Fecha Final.",Color.RED);
            error2Dlg.setVisible(true);
            return false;
        }
        return true;
    }
    
    private TmsServiciosTblFacadeRemote lookupTmsServiciosTblFacade() {
        try {
            Context c = new InitialContext();
            return (TmsServiciosTblFacadeRemote) c.lookup("tms_exerpt.solicitud.TmsServiciosTblFacadeRemote");
        }
        catch(NamingException ne) {
            Logger.getLogger(getClass().getName()).log(Level.SEVERE,"exception caught" ,ne);
            throw new RuntimeException(ne);
        }
    }

