    private void jTxtAutobusKeyPressed(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_jTxtAutobusKeyPressed
        if(evt.getKeyCode() == evt.VK_DOWN)
            jTxtFi.dateEditor.setFoco();
        
        if(evt.getKeyCode() == evt.VK_UP)
            jTxtOperador.requestFocus(); 
    }//GEN-LAST:event_jTxtAutobusKeyPressed

    private void jTxtOperadorKeyPressed(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_jTxtOperadorKeyPressed
        if(evt.getKeyCode() == evt.VK_DOWN)
            jTxtAutobus.requestFocus();
        
        if(evt.getKeyCode() == evt.VK_UP)
            jCmbEmpresa.requestFocus();        
    }//GEN-LAST:event_jTxtOperadorKeyPressed

    private void jCmbEmpresaKeyPressed(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_jCmbEmpresaKeyPressed
        if(evt.getKeyCode() == evt.VK_RIGHT)
            jTxtOperador.requestFocus();
        
        if(evt.getKeyCode() == evt.VK_LEFT)
            jCmbServicio.requestFocus();
    }//GEN-LAST:event_jCmbEmpresaKeyPressed

    private void jCmbServicioKeyPressed(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_jCmbServicioKeyPressed
        if(evt.getKeyCode() == evt.VK_RIGHT)
        {
            if(jCmbEmpresa.isVisible())
              jCmbEmpresa.requestFocus();
            else
                jTxtFi.dateEditor.setFoco();
        }
        
        if(evt.getKeyCode() == evt.VK_LEFT)
            jTxtFf.dateEditor.setFoco();    
    }//GEN-LAST:event_jCmbServicioKeyPressed

