    private void cargarServicios() {
        List<TmsServiciosTbl> servicios = new ArrayList<TmsServiciosTbl>();
        servicios = servicioFacadeRemote.findAll();
        TmsServiciosTbl ser = new TmsServiciosTbl();
        ser.setServicioId(new BigDecimal("9999"));
        ser.setServicioNombre("Todos");
        ser.setDescripcion("Todos");
        servicios.add(0,ser);
        ServiciosComboBoxModel servicioModelo = new ServiciosComboBoxModel(servicios);
        setServiciosModelo(servicioModelo);
    }

    private void setServiciosModelo(ServiciosComboBoxModel servicioModelo) {
        jCmbServicio.setModel(servicioModelo);
        jCmbServicio.setSelectedIndex(0);
    }

    public void keyTyped(KeyEvent e) {
    }

    public void keyPressed(KeyEvent e) {
        if (e.getKeyCode() == KeyEvent.VK_F10){
            if (!validaParametros()) return;                
                ejecutaReporteJasper();
                this.dispose();
        }else if (e.getKeyCode() == KeyEvent.VK_ESCAPE){
            this.dispose();
        }
    }

