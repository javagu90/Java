                    if(!sesionVenta.AuditarFuncion()){
                        DialogoAceptar.mostrarDialogo("¡Error al tratar de auditar la funcion Cancelar Boleto!", "La aplicacion será cerrada.\nContacte al administrador del sistema.",Color.RED);
