                        peticionAC.getFoliosBoletos().add(b);
                    }
                    peticionAC.setSesionId(this.sesionId);
                    System.out.println("Asientos a Enviar: "+peticionAC.getAsientos().size());
                    CanjeBAResp respuestaAC = getCanjeBA(peticionAC);
