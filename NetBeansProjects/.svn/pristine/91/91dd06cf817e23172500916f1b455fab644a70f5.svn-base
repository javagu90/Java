    private void venderBoleto(){
        ERTMSWS_WS.BloquearAsientosReq peticion = new ERTMSWS_WS.BloquearAsientosReq();
        peticion.setClaveCorrida(jTable1.getValueAt(jTable1.getSelectedRow(),0).toString());
        peticion.setModalidad("O");//O => Ocupar(I), L => Liberar(E)
        peticion.setNoAsientos(","+jtxtAsientos.getText().trim());//en cadena separados con ","
        peticion.setOrigen(jcmbxOrigen.getSelectedItem().toString());
        peticion.setSesionId(this.sesionId);
        peticion.setTipoPasajero(","+jtxtTiposPasajero.getText().trim());//en cadena separados con ","
        BloquearAsientosResp respuesta = getBloquearAsientos(peticion);
        System.out.println("************************* Bloquear Asientos *************************");
        System.out.println("respuesta: "+respuesta.isOperacionExitosa());
        System.out.println("mensaje("+respuesta.getErrorCode()+"): "+respuesta.getErrorMsg());
        if(respuesta.isOperacionExitosa()){
            ERTMSWS_WS.VenderBoletosReq peticionVta = new ERTMSWS_WS.VenderBoletosReq();
            String[] arrayAsientos = jtxtAsientos.getText().split(",");
            String[] arrayTipoAsientos = jtxtTiposPasajero.getText().split(",");
             String[] arrayFolios = jtxtfolios.getText().split(",");
            for(int i=0; i<arrayAsientos.length; i++)
            {
                Asientos asiento = new Asientos();
                asiento.setCaja("CAJAWEB");
                asiento.setClaveCorrida(jTable1.getValueAt(jTable1.getSelectedRow(), 0).toString());
                asiento.setDestino(jcmbxDestino.getSelectedItem().toString());
                asiento.setEmpresa(jcmbxEmpresa.getSelectedItem().toString());
                float tarifa = tarifaAdulto;
                if(arrayTipoAsientos[i].equals("M")) tarifa = tarifaMenor;
                if(arrayTipoAsientos[i].equals("S")) tarifa = tarifaSenectud;
                if(arrayTipoAsientos[i].equals("P")) tarifa = tarifaProfesor;
                if(arrayTipoAsientos[i].equals("E")) tarifa = tarifaEstudiante;
                asiento.setImporteBoleto(tarifa);
                asiento.setFechaHoraVenta(getFechaHoy());
                asiento.setNoAsiento(Integer.valueOf(arrayAsientos[i]));
                asiento.setNombrePasajero(jTextField8.getText().trim().equals("")?"VALIDO AL PORTADOR":jTextField8.getText().trim());
                asiento.setOrigen(jcmbxOrigen.getSelectedItem().toString());
                asiento.setOrigenCorrida(jcmbxOrigen.getSelectedItem().toString());
                asiento.setServicio(jcmbxServicio.getSelectedItem().toString().substring(0,1));
                asiento.setSucursalClave("CAPU");
                asiento.setTipoBoleto("S");
                asiento.setTipoPago("");
                asiento.setReferenciaPago("");
                if(jrbtn_redondo.isSelected())
                    asiento.setFolioPreimpreso(arrayFolios[(i*2)]);
                else
                    asiento.setFolioPreimpreso(arrayFolios[i]);
                asiento.setSerieBoleto("0");
                asiento.setClaveReservacion("");
                asiento.setReferenciaAdicional1("");
                asiento.setReferenciaAdicional2("");
                asiento.setReferenciaAdicional3("");
                asiento.setReferenciaAdicional4("");
                asiento.setReferenciaAdicional5("");
                asiento.setReferenciaAdicional6("");
                asiento.setReferenciaAdicional7("");
                asiento.setReferenciaAdicional8("");
                asiento.setReferenciaAdicional9("");
                asiento.setReferenciaAdicional10("");
                asiento.setTipoPasajero(arrayTipoAsientos[i]);
                asiento.setTipoVenta("N");
                asiento.setUsuarioNumero("1003");
                peticionVta.getAsientos().add(asiento);
                if(jrbtn_redondo.isSelected())
                    peticionVta.getAsientos().add(getAsientoRedondo(asiento, arrayFolios[((i*2)+1)]));
            }

            peticionVta.setSesionId(this.sesionId);
            System.out.println("Asientos a Enviar: "+peticionVta.getAsientos().size());
            //System.out.println("Folio: "+peticionVta.getAsientos().get(0).getNoAsiento());
            //System.out.println("Folio: "+peticionVta.getAsientos().get(1).getNoAsiento());
            VenderBoletosResp respuestaVta = getVenderBoletos(peticionVta);
            System.out.println("************************* Vender Boletos *************************");
            System.out.println("respuesta: "+respuestaVta.isOperacionExitosa());
            System.out.println("mensaje("+respuestaVta.getErrorCode()+"): "+respuestaVta.getErrorMsg());
            if(respuestaVta.isOperacionExitosa()){
                JOptionPane.showMessageDialog(this, "Boleto Vendido Correctamente","¡Error!",JOptionPane.ERROR_MESSAGE);
                    int c=0;
                    for(FoliosBoletos f :respuestaVta.getFoliosBoletos())
                    {
                        System.out.println("----------- Boleto Vendido "+(c++)+" -----------");
                        System.out.println("FolioPreimpreso: "+ f.getFolioPreimpreso());
                        System.out.println("SerieBoleto: "+ f.getSerieBoleto());
                        System.out.println("NoAsiento: "+ f.getNoAsiento());
                        System.out.println("NombrePasajero: "+ f.getNombrePasajero());
                    }
            }
            else
                JOptionPane.showMessageDialog(this, respuestaVta.getErrorMsg(),"¡Error!",JOptionPane.ERROR_MESSAGE);
        }
        else
            JOptionPane.showMessageDialog(this, respuesta.getErrorMsg(),"¡Error!",JOptionPane.ERROR_MESSAGE);
    }

