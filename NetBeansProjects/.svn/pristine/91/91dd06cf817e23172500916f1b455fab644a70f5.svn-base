    private void jbtnBuscarCorridasActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtnBuscarCorridasActionPerformed
    DefaultTableModel model = (DefaultTableModel) jTable1.getModel();
    model.setDataVector(null,new String [] {
                "ClaveCorrida", "FechaHoraCorrida", "Origen", "Destino", "Servicio", "Empresa", "Tarifa"
            } );
    CorridasReq peticion = new CorridasReq();
    peticion.setOrigen(jcmbxOrigen.getSelectedItem().toString());
    peticion.setDestino(jcmbxDestino.getSelectedItem().toString().equals("TODOS")?"":jcmbxDestino.getSelectedItem().toString());
    peticion.setEmpresa(jcmbxEmpresa.getSelectedItem().toString().equals("TODAS")?"":jcmbxEmpresa.getSelectedItem().toString());
    peticion.setServicio(jcmbxServicio.getSelectedItem().toString().equals("TODOS")?"":jcmbxServicio.getSelectedItem().toString().substring(0,1));
    peticion.setSesionId(this.sesionId);
    if(jtxtFechaCorrida.getDateEditor().getTexto().trim().equals(""))
        return ;
        GregorianCalendar gc = new GregorianCalendar();
        //gc.setTimeInMillis(jtxtFechaCorrida.getDate().getTime());
        XMLGregorianCalendar cal = null;
        try {
            cal = DatatypeFactory.newInstance().newXMLGregorianCalendar(gc);
        } catch (DatatypeConfigurationException ex) {
            Logger.getLogger(JFTMSVenta.class.getName()).log(Level.SEVERE, null, ex);
        }
        String[] array = jtxtFechaCorrida.getDateEditor().getTexto().split("/");
        cal.setDay(Integer.valueOf(array[0]));
        cal.setMonth(Integer.valueOf(array[1]));
        cal.setYear(Integer.valueOf(array[2]));
        if(jtxtHora.getText().trim().equals(""))
        {
            cal.setHour(0);
            cal.setMinute(0);
        }
         else
        {
            String[] arrayh = jtxtHora.getText().split(":");
            cal.setHour(Integer.valueOf(arrayh[0]));
            cal.setMinute(Integer.valueOf(arrayh[1]));
        }
        peticion.setFechaHoraSalida(cal);

    CorridasResp respuesta =  getCorridas(peticion);
    System.out.println("************************* Buscar Corrodas *************************");
    System.out.println("respuesta: "+respuesta.isOperacionExitosa());
    System.out.println("mensaje("+respuesta.getErrorCode()+"): "+respuesta.getErrorMsg());

        if(respuesta.isOperacionExitosa()){
            System.out.println("Corridas: "+respuesta.getCorridas().size());
            System.out.println("    "+respuesta.getCorridas());
            String[] row = new String[7];
            for(ERTMSWS_WS.Corrida c : respuesta.getCorridas())
            { 
              row[0]=c.getClaveCorrida();
              row[1]=c.getFechaCorrida().getDay()+"/"+c.getFechaCorrida().getMonth()+"/"+c.getFechaCorrida().getYear()+" "+c.getHoraCorrida().getHour()+":"+c.getHoraCorrida().getMinute();
              row[2]=c.getOrigenCorrida();
              row[3]=c.getDestinoCorrida();
              row[4]=c.getServicio();
              row[5]=c.getEmpresa();
              String[] arrayt = c.getTarifaSencillo().split(",");
              try{
                 row[6]=arrayt[0];
                 tarifaAdulto = Float.valueOf(arrayt[0]);
                 tarifaMenor = Float.valueOf(arrayt[1]);
                 tarifaSenectud = Float.valueOf(arrayt[2]);
                 tarifaProfesor = Float.valueOf(arrayt[3]);
                 tarifaEstudiante = Float.valueOf(arrayt[4]);
                }catch(java.lang.ArrayIndexOutOfBoundsException ex)
                {
                    row[6]="-1";
                     tarifaAdulto = -1;
                     tarifaMenor = -1;
                     tarifaSenectud = -1;
                     tarifaProfesor = -1;
                     tarifaEstudiante = -1;
                }
              model = (DefaultTableModel) jTable1.getModel();
              model.addRow(row);
            }
        }
        else
            JOptionPane.showMessageDialog(this, respuesta.getErrorMsg(),"¡Error!",JOptionPane.ERROR_MESSAGE);

    }//GEN-LAST:event_jbtnBuscarCorridasActionPerformed

