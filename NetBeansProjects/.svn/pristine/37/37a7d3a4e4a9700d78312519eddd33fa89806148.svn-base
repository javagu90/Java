package vendERMobile.clases.parametrosIniciales;

/**
 *
 * @author asolis
 */
public class ParametrosInicialesRequest {
    String uid;
    String campoTipo;
    String cajaNombre;
    /** Creates a new instance of ParametrosInicialesResponse */
    public ParametrosInicialesRequest() {
    }
    
    public void setUid(String uid){
        this.uid = uid;
    }
    public String getUid(){
        return this.uid;
    }
    
    public void setCampoTipo(String campoTipo){
        this.campoTipo = campoTipo;
    }
    public String getCampoTipo(){
        return this.campoTipo;
    }
    
    public void setCajaNombre(String cajaNombre){
        this.cajaNombre = cajaNombre;
    }
    public String getCajaNombre(){
        return this.cajaNombre;
    }
}
