<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:f="http://xmlns.jcp.org/jsf/core"
      xmlns:p="http://primefaces.org/ui"      
      xmlns:pos="http://pos.com/functions">
	
	<ui:composition template="/template/template.xhtml">
		<ui:define name="body">
			<h:form id="CorteParcialListForm">
            	<p:layout
	            	id="corteParcialLayout"
	            	style="height:auto;" >
		            	<p:layoutUnit 
		            		id="corteParcialWestLayout" 
		            		position="west" 
		            		size="150" 
		            		header="Menu" 
		            		collapsible="true"
		            		style="height:auto;" >
		            		<p:remoteCommand 
		            			name="updateButtons" 
		            			update="nuevoCorteParcialButton" />	
		            		<p:commandButton 			            	    
		                		id="nuevoCorteParcialButton"
		                		icon="ui-icon-plus"   
		                		value="#{msg['catalogo.LeftMenu.Button.Nuevo']}"
		                		style="width:98%;height:50px" 
		                		onclick="PF('VentanaCPCreado').show();"/>
		   			</p:layoutUnit>
		            <p:layoutUnit 
		            	id="corteParcialCenterLayout"
		            	position="center"
		            	style="height:auto;">
		            	
		            	<p:panel id="corteParcialListPanel"
        						header="Corte Parcial">
	        					<p:panelGrid
	       							columnClasses="columnPanelGrid"
		            				id="perfilFilters">
		            				<p:row>		
			            				<p:column>
			            					<h:outputText
					            				styleClass="boldLabel" 
					            				value="Nombre" />
				            				<div/>
				            				<p:inputText styleClass="centerLabel" 
					           					style="width: 150px;"
					           					value="n" >
					           					<f:validateLength maximum="100"/>
					           				</p:inputText>
				            			</p:column>
				            			<p:column>
				            				<div/>
				            				<p:commandButton
				            					icon="ui-icon-search"
				            					value="Buscar"
				            					update="corteParcialListPanel"
				            					/>
				            			</p:column>
				            		</p:row>
		            			</p:panelGrid>
							<p:dataTable 
		                    	id="datalist" 
		                    	value="#{corteParcialBean.listaCorteParcial}"
		                    	var="lista"
		                    	paginator="true"
		                    	paginatorPosition="bottom">

							<p:column headerText="Id">
        						<h:outputText value="#{lista.idcorteparcial}" />
    						</p:column>
 
    						<p:column headerText="Id Usuario">
        						<h:outputText value="#{lista.usuario1}" />
    						</p:column>
 
    						<p:column headerText="Id Supervisor">
        						<h:outputText value="#{lista.usuario2}" />
    						</p:column>
 
    						<p:column headerText="Id Corte de caja">
        						<h:outputText  value="#{lista.cortecaja}"/>
    						</p:column>
							
    						<p:column headerText="Fecha">
        						<h:outputText value="#{lista.fecha}" />
    						</p:column>
    						
    						
    						<p:column headerText="Hora">
        						<h:outputText value="#{lista.hora}" />
    						</p:column>
    						
    						
    						<p:column headerText="Cantidad Retirada">
        						<h:outputText  value="#{lista.cantidadretirada}"/>
    						</p:column>
    						
    						
    						<p:column headerText="Comentarios">
        						<h:outputText value="#{lista.comentarios}" />
    						</p:column>
    						
    						
    						<p:column headerText="Ultima Venta">
        						<h:outputText  value="#{lista.ultimaventa}"/>
    						</p:column>
							
							
    						<p:column headerText="Ultimo pago Apartado">
        						<h:outputText value="#{lista.ultimopagoapartado}" />
    						</p:column>
							
							</p:dataTable>
			                </p:panel>
		            	
		            </p:layoutUnit>		            		
				</p:layout>
			</h:form>
        </ui:define>
	</ui:composition>
	
