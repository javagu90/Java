        							<p:column colspan="2">
        								<p:outputLabel id="labelSimbolo7" 
        									value="$ "/>            	
        								<p:inputText id="numeroRetiro"  
        									label="labelRetiro" 
        									required="true"><!-- value="#{validationView.integer}" -->
                							<f:validateDoubleRange minimum="5" maximum="8" />
            							</p:inputText>
        							</p:column>
     							</p:row>
    							<p:row>
    								<p:column colspan="2">
    									<p:outputLabel id="labelVales" 
    										value="Vales"/>
