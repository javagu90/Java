            							<p:outputLabel id="hora" value="Hora:" style="font-weight: bold;"/>
            							<p:outputLabel id="labelHora" value="#{corteParcialBean.stringHora}"/>
            						</p:column>
            						<p:column colspan="4" >
            							<p:outputLabel id="caja" 
            								value="Caja:" 
